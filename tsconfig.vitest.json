{
  "compilerOptions": {
    "noEmit": true,
    "outDir": "./dist/lib",
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "ES2022"
    ],
    "sourceMap": false,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules",
      "./node_modules/@types",
      "./node_modules/@types/assert",
      "./dtslint"
    ],
    "types": [
      "vitest/globals"
    ],
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "include": [
    "./test"
  ],
  "exclude": [
    "./node_modules"
  ]
}
